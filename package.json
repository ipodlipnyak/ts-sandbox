{
  "name": "ts-sandbox",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "fixtures": "fixtures-ts-node-esm load ./apps/template/fixtures --dataSource=dist/apps/template/ormconfig.js",
    "typeorm": "typeorm-ts-node-esm -d dist/apps/template/ormconfig.js",
    "typeorm:migration:generate": "npm run typeorm -- migration:generate ./src/migrations/migration",
    "typeorm:migration:run": "npm run typeorm -- migration:run",
    "typeorm:migration:revert": "npm run typeorm -- migration:revert",
    "prebuild": "rimraf dist",
    "build": "nest build --tsc",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start --tsc",
    "start:dev": "nest start --watch --tsc",
    "start:debug": "nest start --debug --watch --tsc",
    "start:prod": "node dist/apps/template/main",
    "cli": "node dist/apps/template/cli",
    "cli:debug": "node --inspect-brk dist/apps/template/cli",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/baikal/test/jest-e2e.json",
    "app:alfa:test:e2e": "jest --config ./apps/alfa/test/jest-e2e.json",
    "app:alfa:dev:server": "nest start --watch --tsc alfa",
    "app:alfa:debug:server": "nest start --debug --watch --tsc alfa",
    "app:alfa:dev:client": "npm --workspace alfa-client run dev",
    "app:alfa:dev": "npm run app:alfa:dev:server & npm run app:alfa:dev:client",
    "app:alfa:build:server": "nest build --tsc alfa",
    "app:alfa:build:client": "npm run -w alfa-client generate",
    "app:alfa:build": "npm run app:alfa:build:server & npm run app:alfa:build:client",
    "app:alfa:prod": "node dist/apps/alfa/src/main",
    "app:alfa:cli": "node dist/apps/alfa/src/cli",
    "app:alfa:cli:debug": "node --inspect-brk dist/apps/alfa/src/cli",
    "app:alfa:typeorm": "typeorm-ts-node-esm -d dist/apps/alfa/src/ormconfig.js",
    "app:alfa:typeorm:generate": "npm run app:alfa:typeorm -- migration:generate ./apps/alfa/src/migrations/migration",
    "app:alfa:typeorm:run": "npm run app:alfa:typeorm -- migration:run",
    "app:alfa:typeorm:revert": "npm run app:alfa:typeorm -- migration:revert",
    "app:alfa:fixtures": "fixtures-ts-node-esm load apps/alfa/fixtures --dataSource=dist/apps/alfa/src/ormconfig.js"
  },
  "dependencies": {
    "@nestjs/axios": "0.1.0",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/serve-static": "^3.0.0",
    "@nestjs/swagger": "^6.0.0",
    "@nestjs/typeorm": "^9.0.0",
    "@sentry/node": "^7.10.0",
    "@types/express-session": "^1.17.4",
    "bcrypt": "^5.0.1",
    "cache-manager": "^4.0.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "connect-redis": "^6.1.3",
    "cookie-parser": "^1.4.6",
    "express-session": "^1.17.3",
    "nest-commander": "^3.1.0",
    "pg": "^8.7.3",
    "redis": "^4.1.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "sinon": "^14.0.0",
    "swagger-ui-express": "^4.4.0",
    "ts-node": "^10.8.0",
    "typeorm": "^0.3.6",
    "typeorm-fixtures-cli": "^3.0.0",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cache-manager": "^3.4.3",
    "@types/cache-manager-redis-store": "^2.0.1",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/sinon": "^10.0.13",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "tsconfig-paths": "4.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@cg/sentry(|/.*)$": "<rootDir>/libs/sentry/src/$1"
    }
  },
  "workspaces": [
    "packages/*",
    "apps/alfa/client",
    "apps/baikal/client",
    "apps/template/client"
  ]
}
