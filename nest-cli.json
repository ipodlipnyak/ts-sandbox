{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/template/src",
  "monorepo": true,
  "root": "apps/template",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/template/tsconfig.app.json"
  },
  "projects": {
    "sentry": {
      "type": "library",
      "root": "libs/sentry",
      "entryFile": "index",
      "sourceRoot": "libs/sentry/src",
      "compilerOptions": {
        "tsConfigPath": "libs/sentry/tsconfig.lib.json"
      }
    },
    "google": {
      "type": "library",
      "root": "libs/google",
      "entryFile": "index",
      "sourceRoot": "libs/google/src",
      "compilerOptions": {
        "tsConfigPath": "libs/google/tsconfig.lib.json"
      }
    },
    "llm": {
      "type": "library",
      "root": "libs/llm",
      "entryFile": "index",
      "sourceRoot": "libs/llm/src",
      "compilerOptions": {
        "tsConfigPath": "libs/llm/tsconfig.lib.json"
      }
    },
    "users": {
      "type": "library",
      "root": "libs/users",
      "entryFile": "index",
      "sourceRoot": "libs/users/src",
      "compilerOptions": {
        "tsConfigPath": "libs/users/tsconfig.lib.json"
      }
    },
    "template": {
      "type": "application",
      "root": "apps/template",
      "entryFile": "main",
      "sourceRoot": "apps/template/src",
      "compilerOptions": {
        "tsConfigPath": "apps/template/tsconfig.app.json"
      }
    },
    "bubbles": {
      "type": "application",
      "root": "apps/bubbles",
      "entryFile": "main",
      "sourceRoot": "apps/bubbles/src",
      "compilerOptions": {
        "tsConfigPath": "apps/bubbles/tsconfig.app.json"
      }
    }
  }
}
