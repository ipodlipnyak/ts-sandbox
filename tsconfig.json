{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2018",
    "lib": ["es2018", "esnext.asynciterable"],
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "noImplicitThis": true,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false,
    "paths": {
      "@my/sentry": [
        "libs/sentry/src"
      ],
      "@my/sentry/*": [
        "libs/sentry/src/*"
      ],
      "@my/google": [
        "libs/google/src"
      ],
      "@my/google/*": [
        "libs/google/src/*"
      ],
      "@my/llm": [
        "libs/llm/src"
      ],
      "@my/llm/*": [
        "libs/llm/src/*"
      ],
      "@my/users": [
        "libs/users/src"
      ],
      "@my/users/*": [
        "libs/users/src/*"
      ]
    }
  }
}
